<div class="row">
  <div class="col-md-12 text-center title-brand">
      <h1>Brands</h1>
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <input type="text" class="form-control">
      </div>
    </div>

    <div class="row">
      <div class="col-md-2 col-md-offset-5">
          <button class="btn btn-danger btn-block btn-lg btn-search">Search</button>
      </div>
    </div>

  </div>
</div>



<div class="row">
  <div class="col-md-12 text-center center-box">
    <% @brands.each do |brand| %>
    <ul>
        <li><h2><%= link_to brand.name, scrapper_path(brand.id) %></h2></li>
        </ul>
    <% end %>
  </div>
</div>
